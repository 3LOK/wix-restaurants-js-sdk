{
  "name": "wix-restaurants-js-sdk",
  "version": "1.0.0",
  "description": "Wix Restaurants JavaScript SDK",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require test/setup.js \"test/**/*.test.js\"",
    "lint": "eslint ./src ./test",
    "posttest": "npm run lint",
    "compile": "babel -d dist/ src/",
    "prepublish": "npm test && npm run compile",
    "build": ":",
    "release": "npm install @wix/wnpm-ci && wnpm-release -- --no-shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/wix-restaurants-js-sdk.git"
  },
  "keywords": [
    "wix",
    "restaurants"
  ],
  "author": "Wix Restaurants",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wix/wix-restaurants-js-sdk/issues"
  },
  "homepage": "https://github.com/wix/wix-restaurants-js-sdk",
  "dependencies": {
    "q": "^1.4.1",
    "availability": "^2.1.254",
    "moment": "^2.15.2",
    "lodash": "^4.8.2",
    "moment-timezone": "^0.5.7",
    "babel-runtime": "^6.18.0",
    "axios": "^0.15.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-strict-mode": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.9.0",
    "http": "0.0.0",
    "mocha": "^2.4.5",
    "nock": "^9.0.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
